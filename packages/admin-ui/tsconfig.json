{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strict": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "target": "es2015",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "node_modules/@types",
      "./typings"
    ],
    "lib": [
      "es2019",
      "dom",
      "esnext.asynciterable"
    ],
    "paths": {
      "@uplab/admin-ui": [
        "./src/lib/public_api"
      ],
      "@uplab/admin-ui/catalog": [
        "./src/lib/catalog/src/public_api"
      ],
      "@uplab/admin-ui/core": [
        "./src/lib/core/src/public_api"
      ],
      "@uplab/admin-ui/customer": [
        "./src/lib/customer/src/public_api"
      ],
      "@uplab/admin-ui/dashboard": [
        "./src/lib/dashboard/src/public_api"
      ],
      "@uplab/admin-ui/login": [
        "./src/lib/login/src/public_api"
      ],
      "@uplab/admin-ui/marketing": [
        "./src/lib/marketing/src/public_api"
      ],
      "@uplab/admin-ui/order": [
        "./src/lib/order/src/public_api"
      ],
      "@uplab/admin-ui/settings": [
        "./src/lib/settings/src/public_api"
      ],
      "@uplab/admin-ui/system": [
        "./src/lib/system/src/public_api"
      ]
    }
  }
}
